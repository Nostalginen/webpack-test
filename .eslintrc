{
  "env": {
    "browser": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-console": 1,
    "no-unused-vars": 1,
    "react/prop-types": 1,
    "prettier/prettier": 1,
    "jsx-a11y/aria-role": 0,
    "jsx-a11y/aria-props": 0,
    "jsx-a11y/no-static-element-interactions": 0,
    "jsx-a11y/click-events-have-key-events": 0
  },
  "extends": [
    "plugin:prettier/recommended",
    "plugin:react/recommended",
    "eslint:recommended",
    "plugin:jsx-a11y/recommended",
    "prettier/react"
  ],
  "plugins": ["jsx-a11y"],
  "settings": {
    "react": {
      "version": "16.5.2"
    }
  }
}
