{
  "name": "webpack-config",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "browserslist": {
    "production": [
      "> 1%",
      "last 2 versions",
      "IE 10"
    ],
    "development": [
      "last 2 versions"
    ]
  },
  "scripts": {
    "start": "webpack-dev-server --open --color --hot --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "eslint": "eslint --fix ./src",
    "prettier": "prettier --write 'src/**/*.{js,scss}'",
    "pretty-quick": "pretty-quick --staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick && yarn eslint"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.3.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-preset-mobx": "^2.0.0",
    "browserslist": "^4.3.3",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "cssnano": "^4.1.7",
    "dotenv-webpack": "^1.5.7",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.1.2",
    "node-sass": "^4.9.4",
    "postcss-loader": "^3.0.0",
    "postcss-pxtorem": "^4.0.1",
    "prettier": "1.13.7",
    "pretty-quick": "^1.8.0",
    "sass-loader": "^7.1.0",
    "sass-resources-loader": "^1.3.4",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.0",
    "svg-loader": "^0.0.2",
    "url-loader": "^1.1.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "mobx": "4.5.1",
    "mobx-react": "^5.3.5",
    "mobx-utils": "^5.0.4",
    "prop-types": "^15.6.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-intl": "^2.7.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1"
  }
}
